{
  "expo": {
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/icon.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ericphung.financely",
      "requireFullScreen": true,
      "isTabletOnly": false,
      "infoPlist": {
         "NSPhotoLibraryUsageDescription": "Financely needs access to use photos you select."
      }
    },
    "name": "Financely",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "slug": "budgetx-app",
    "splash": {
      "image": "./assets/splashscreen-budgetx.png",
      "resizeMode": "cover",
      "backgroundColor": "#18202f",
      "userInterfaceStyle": "dark"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "version": "1.0.5"
  }
}